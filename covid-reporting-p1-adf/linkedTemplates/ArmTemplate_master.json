{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-reporting-p1-adf"
		},
		"ls_ablob_covidreportingsatry1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidreportingsatry1'"
		},
		"ls_adls_covidreportingdltry1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportingdltry1'"
		},
		"ls_adls_covidreportingdltry1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportingdltry1.dfs.core.windows.net"
		},
		"ls_https_opendata_ecdc_europe_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().SourceBaseURL}"
		},
		"trigger_population_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/0fce3677-9edb-4ac1-8eaf-ce177c3665fc/resourceGroups/covid-reporting-rg/providers/Microsoft.Storage/storageAccounts/covidreportingsatry1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-reporting-p1-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidreportingsatry1_connectionString": {
						"value": "[parameters('ls_ablob_covidreportingsatry1_connectionString')]"
					},
					"ls_adls_covidreportingdltry1_accountKey": {
						"value": "[parameters('ls_adls_covidreportingdltry1_accountKey')]"
					},
					"ls_adls_covidreportingdltry1_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportingdltry1_properties_typeProperties_url')]"
					},
					"ls_https_opendata_ecdc_europe_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_https_opendata_ecdc_europe_eu_properties_typeProperties_url')]"
					},
					"trigger_population_properties_typeProperties_scope": {
						"value": "[parameters('trigger_population_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}